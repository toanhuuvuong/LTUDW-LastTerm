.container-fluid
  .row.text-center
    .col-lg-12.col-md-12.col-sm-12.col-xs-12
      .review-tab-pro-inner(styte='margin-top: 100px;')
        form(action='login.html', method='POST')
          ul#myTab3.tab-review-design
            li.active
              a(href='#')
                i
                |  Đăng nhập
          #myTabContent.tab-content.custom-product-edit
            #description.product-tab-list.tab-pane.fade.active.in
              .row
                .review-content-section
                  if success_msg!=''
                    .alert.alert-success
                      | #{success_msg}
                  if error_msg!=''
                    .alert.alert-danger
                      | #{error_msg}
                  if error!=''
                    .alert.alert-danger
                      | #{error}
                  .input-group.mg-b-pro-edt
                    span.input-group-addon
                      i.icon.nalika-edit(aria-hidden='true')
                    input.form-control(type='email', name='email', placeholder='Email')
                  .input-group.mg-b-pro-edt
                    span.input-group-addon
                      i.icon.nalika-edit(aria-hidden='true')
                    input.form-control(type='password', name='password', placeholder='Mật khẩu')
              .row
                .text-center.custom-pro-edt-ds
                  button.btn.btn-ctl-bt.waves-effect.waves-light.m-r-10(type='submit', value='submit')
                    | Đăng nhập
